                                                .controls
                                                        select(ui-select2, multiple="multiple", name="backUpDocumentRequirements", ng-model="account.backUpDocumentRequirements", data-placeholder="Back-Up Document Requirements", style="width:220px")
                                                                option(ng-repeat="row in backUpDocumentRequirementsList", value="{{row.v}}") {{row.n}}
