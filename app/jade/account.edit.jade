.offset1.span8
        form.form-horizontal(novalidate, name="form")
                .fieldset
                        .fluid-row
                                .pull-right
                                        a.btn.btn-primary.btn-margin(ng-hide="accountUpdateLoader", ng-click="backToDetails()", style="cursor: pointer;", bs-tooltip="tooltip.back")
                                                i.icon-mail-reply.icon-large
                                        button.btn.btn-primary(permission="updateAccount.*.*", ng-hide="accountUpdateLoader", ng-disabled="form.$invalid", ng-click="updateAccount(account)", style="cursor: pointer;", bs-tooltip="tooltip.save")
                                                i.icon-ok.icon-large
                                        a.btn.btn-primary.btn-margin(permission="deleteAccount.*.*", ng-hide="accountUpdateLoader", style="cursor: pointer;", bs-tooltip="tooltip.delete"  , ng-click="confirmDeleteAccount()")
                                                i.icon-trash.icon-large
                                        i.icon-spinner.icon-spin.icon-2x(ng-show="accountUpdateLoader")
                        tabs
                                pane(heading="Account",style="overflow:hidden")
                                        .control-group
                                                label.control-label Account Name
                                                .controls
                                                        input(type="text", name="accountName", duplicate="account", ng-model="account.accountName", placeholder="Account Name",style="width:200px", required)
                                                        label.help-inline.required(ng-show="form.accountName.$error.required") *
                                                        label.help-inline.required(ng-show="form.accountName.$error.duplicate") Account exists.
                                        .control-group
                                                label.control-label Account Codes
                                                .controls
                                                        input(type="text", name="accountCodes", duplicate="accountCode", ng-model="account.accountCodes", placeholder="Account Codes",style="width:200px", required)
                                                        label.help-inline.required(ng-show="form.accountCodes.$error.required") *
                                                        label.help-inline.required(ng-show="form.accountCodes.$error.duplicate") Account code exists.
                                        .control-group
                                                label.control-label  Account Type
                                                .controls
                                                        select(ui-select2, name="accountType", ng-model="account.accountType", data-placeholder="Account Type", style="width:220px", required)
                                                                option(value="")
                                                                option(ng-repeat="row in accountTypeList", value="{{row.v}}") {{row.n}}
                                                        label.help-inline.required(ng-show="form.accountType.$error.required") *
                                        .control-group(ng-show="account.accountType=='INTERNATIONAL'")
                                                label.control-label Shipment Modes
                                                .controls
                                                        select(ui-select2, multiple="multiple", name="shipmentModes", ng-model="account.shipmentModes", data-placeholder="Shipment Modes", style="width:220px")
                                                                option(ng-repeat="row in shipmentModesList", value="{{row.v}}") {{row.n}}
                                                        //- label.help-inline.required(ng-show="form.shipmentModes.$error.required") *
                                        .control-group
                                                label.control-label  Shipment Volume
                                                .controls
                                                        input(type="text", name="shipmentVolume", ng-model="account.shipmentVolume", placeholder="Shipment Volume", style="width:205px", required)
                                                        label.help-inline.required(ng-show="form.shipmentVolume.$error.required") *
                                        .control-group
                                                label.control-label  AR Booked in
                                                .controls
                                                        select(ui-select2, name="ARBookedIn", ng-model="account.ARBookedIn", data-placeholder="AR Booked In", style="width:220px", required)
                                                                option(value="")
                                                                option(ng-repeat="row in ARBookedInList", value="{{row.v}}") {{row.n}}
                                                        label.help-inline.required(ng-show="form.ARBookedIn.$error.required") *
                                        .control-group
                                                label.control-label  Invoicing Type
                                                .controls
                                                        select(ui-select2, name="invoicingType", ng-model="account.invoicingType", data-placeholder="Invoicing Type", style="width:220px", required)
                                                                option(value="")
                                                                option(ng-repeat="row in invoicingTypeList", value="{{row.v}}") {{row.n}}
                                                        label.help-inline.required(ng-show="form.invoicingType.$error.required") *
                                        .control-group
                                                label.control-label Rating Type
                                                .controls
                                                        select(ui-select2, name="ratingType", ng-model="account.ratingType", data-placeholder="Rating Type", style="width:220px", required)
                                                                option(value="")
                                                                option(ng-repeat="row in ratingTypeList", value="{{row.v}}") {{row.n}}
                                                        label.help-inline.required(ng-show="form.ratingType.$error.required") *
                                        .control-group(ng-show="account.ratingType=='TARIFF'")
                                                label.control-label Currency Conversion Applicable
                                                .controls
                                                        select(ui-select2, name="currencyConversionApplicable", ng-model="account.currencyConversionApplicable", data-placeholder="Currency Conversion Applicable", style="width:220px")
                                                                option(value="")
                                                                option(ng-repeat="row in currencyConversionApplicableList", value="{{row.v}}") {{row.n}}
                                                        //- label.help-inline.required(ng-show="form.currencyConversionApplicable.$error.required") *
                                        .control-group(ng-show="account.ratingType=='TARIFF'")
                                                label.control-label Sources Of Currency Conversation
                                                .controls
                                                        select(ui-select2, multiple="multiple", name="sourcesOfCurrencyConversation", ng-model="account.sourcesOfCurrencyConversation", data-placeholder="Sources Of Currency Conversation", style="width:220px")
                                                                option(ng-repeat="row in sourcesOfCurrencyConversationList", value="{{row.v}}") {{row.n}}
                                                        //- label.help-inline.required(ng-show="form.sourcesOfCurrencyConversation.$error.required") *
                                        .control-group
                                                label.control-label Any Limitation On # Of Days To Invoice
                                                .controls
                                                        select(ui-select2, name="anyLimitationOnNoOfDaysToInvoice", ng-model="account.anyLimitationOnNoOfDaysToInvoice", data-placeholder="Any Limitation On # Of Days To Invoice", style="width:220px", required)
                                                                option(value="")
                                                                option(ng-repeat="row in anyLimitationOnNoOfDaysToInvoiceList", value="{{row.v}}") {{row.n}}
                                                        label.help-inline.required(ng-show="form.anyLimitationOnNoOfDaysToInvoice.$error.required") *
                                        .control-group(ng-show="account.anyLimitationOnNoOfDaysToInvoice=='YES'")
                                                label.control-label Criteria
                                                .controls
                                                        select(ui-select2, name="criteria", ng-model="account.criteria", data-placeholder="Criteria", style="width:220px")
                                                                option(value="")
                                                                option(ng-repeat="row in criteriaList", value="{{row.v}}") {{row.n}}
                                                        //- label.help-inline.required(ng-show="form.noOfDaysToInvoice.$error.required") *
                                        .control-group(ng-show="account.anyLimitationOnNoOfDaysToInvoice=='YES'")
                                                label.control-label # Of Days
                                                .controls
                                                        select(ui-select2, name="noOfDaysToInvoice", ng-model="account.noOfDaysToInvoice", data-placeholder="# Of Days", style="width:220px")
                                                                option(value="")
                                                                option(ng-repeat="row in noOfDaysToInvoiceList", value="{{row.v}}") {{row.n}}
                                                        //- label.help-inline.required(ng-show="form.noOfDaysToInvoice.$error.required") *
                                        .control-group
                                                label.control-label Workflow Mode
                                                .controls
                                                        select(ui-select2="{allowClear: true}", ng-change="getWokflowModeValues(account.workflowMode)", name="workflowMode", ng-model="account.workflowMode", data-placeholder="Please select...", style="width:220px")
                                                                option(value="")
                                                                option(ng-repeat="workflowMode in workflowModeList", value="{{workflowMode.wmid}}") {{workflowMode.n}}
                                                        //- label.help-inline.required(ng-show="form.workflowMode.$error.required") *
                                        div(ng-switch="account.workflowMode")
                                                .control-group(ng-switch-when="1")
                                                        label.control-label Select Workflow
                                                        .controls
                                                                input.span4(ui-select2="workflowBar", name="workflowBar", ng-model="account.workflow")
                                                                //- .help-inline.required(ng-show="form.workflowBar.$error.required") *
                                                .control-group(ng-switch-when="2")
                                                        label.control-label Select Group
                                                        .controls
                                                                input.span4(ui-select2="groupBar", name="groupBar", ng-model="account.tag", required)
                                                                .help-inline.required(ng-show="form.groupBar.$error.required") *
                                                .control-group(ng-switch-when="3")
                                                        label.control-label Add Condition
                                                        .controls
                                                                input(type="text", name="condition", ng-model="account.condition", required)
                                                                .help-inline.required(ng-show="form.condition.$error.required") *
