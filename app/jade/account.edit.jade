                                                label.control-label Is FPC Applicable?
                                                .controls
                                                        select(ui-select2, name="isFpcApplicable", ng-model="account.isFpcApplicable", data-placeholder="Is FPC Applicable?", style="width:220px", required)
                                                                option(value="")
                                                                option(ng-repeat="row in isFpcApplicableList", value="{{row.v}}") {{row.n}}
                                                        label.help-inline.required(ng-show="form.isFpcApplicable.$error.required") *

                                        .control-group(ng-show="account.isFpcApplicable=='YES'")
                                                label.control-label  Name of FPC
                                                .controls
                                                        input(type="text",name="fpcName", ng-model="account.fpcName", placeholder="Name of FPC", style="width:205px")
                                                        //- label.help-inline.required(ng-show="form.fpcName.$error.required") *
                                        .control-group(ng-show="account.isFpcApplicable=='YES'")
                                                label.control-label  Website Link
                                                .controls
                                                        input(type="url", name="fpcWebsiteLink", ng-model="account.fpcWebsiteLink", placeholder="Website Link", style="width:205px")
                                                        //- label.help-inline.required(ng-show="form.fpcWebsiteLink.$error.required") *
                                                        label.help-inline.required(ng-show="form.fpcWebsiteLink.$error.url")   invalid url
                                        .control-group(ng-show="account.isFpcApplicable=='YES'")
                                                label.control-label  Login ID (If applicable)
                                                .controls
                                                        input(type="text", name="fpcLoginId", ng-model="account.fpcLoginId", placeholder="Login ID", style="width:205px")                                                        
                                        .control-group(ng-show="account.isFpcApplicable=='YES'")
                                                label.control-label  Password (if applicable)
                                                .controls
                                                        input(type="text", name="fpcPassword", ng-model="account.fpcPassword", placeholder="Password", style="width:205px")                                                        
                                        .control-group
                                                label.control-label Approval Notification Format
                                                .controls
                                                        select(ui-select2, name="approvalNotificationFormat", ng-model="account.approvalNotificationFormat", data-placeholder="Approval Notification Format", style="width:220px",)
                                                                option(value="")
                                                                option(ng-repeat="row in approvalNotificationFormatList", value="{{row.v}}") {{row.n}}
