                                                        select(ui-select2, multiple="multiple", name="invoiceDeliveryMode", ng-model="account.invoiceDeliveryMode", data-placeholder="Invoice Delivery Mode", style="width:220px", required)                                                                
                                                                option(ng-repeat="row in invoiceDeliveryModeList", value="{{row.v}}") {{row.n}}
                                                        label.help-inline.required(ng-show="form.invoiceDeliveryMode.$error.required") *
