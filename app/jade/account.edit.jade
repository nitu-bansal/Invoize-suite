                                pane(heading="Contacts",style="overflow:hidden")
                                        .control-group
                                                .span6 Role
                                                .span6 User
                                        ng-form(name="permissionForm", ng-repeat="row in account.role")
                                                .row-fluid
                                                        .span6
                                                                input.span9(ng-disabled="$index == 0", ui-select2="roleBar", ng-click="rolesCallback($index)", name="roleBar", ng-model="account.role[$index]", required)
                                                                .help-inline.required(ng-show="form.roleBar.$error.required") *
                                                        .span6
                                                                input.span9(ui-select2="userBar", ng-click="rolesCallback($index)", name="userBar", ng-model="account.user[$index]", required)
                                                                .help-inline.required(ng-show="form.userBar.$error.required") *
                                                                .help-inline
                                                                        a.close(ng-hide="$index == 0", ng-click="removeRoles($index)") &times;
