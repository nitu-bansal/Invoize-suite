                                                label.control-label  Invoice Delivery Frequency
                                                .controls
                                                        select(ui-select2, name="invoiceDeliveryFrequency", ng-model="account.invoiceDeliveryFrequency", data-placeholder="Invoice Delivery Frequency", style="width:220px")
