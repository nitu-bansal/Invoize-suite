                                pane(heading="Collections",style="overflow:hidden")
                                        .control-group
                                                label.control-label  Payment Mode
                                                .controls
                                                        select(ui-select2, name="paymentMode", ng-model="account.paymentMode", placeholder="Payment Mode", style="width:205px")
                                                                option(value="")
                                                                option(ng-repeat="row in paymentModeList", value="{{row.v}}") {{row.n}}
                                                                label.help-inline.required(ng-show="form.paymentMode.$error.required") *
