.offset1.span8
        form.form-horizontal(novalidate, name="form")
                fieldset
                        .fluid-row
                                .pull-right
                                        a.btn.btn-primary.btn-margin(ng-hide="organizationCreateLoader", onClick="window.history.back()", style="cursor: pointer;", bs-tooltip="tooltip.back")
                                                i.icon-mail-reply.icon-large
                                        button.btn.btn-primary(ng-hide="organizationCreateLoader", ng-disabled="form.$invalid", type="submit", ng-click="create(org)", style="cursor: pointer;", bs-tooltip="tooltip.save")
                                                i.icon-ok.icon-large
                                        i.icon-spinner.icon-spin.icon-2x(ng-show="organizationCreateLoader")
                                legend Organization Details :
                        .control-group
                                label.control-label Organization
                                .controls
                                        input(type="text", name="organization", ng-model="org.organization", duplicate="organization", placeholder="Organization", required)
                                        label.help-inline.required(ng-show="form.organization.$error.required") *
                                        label.help-inline.required(ng-show="form.organization.$error.duplicate") Organization exists.
                        .control-group
                                label.control-label Domain
                                .controls
                                        .input-append
                                                input.span6(type="text", placeholder="Domain", ng-model="register.subDomain", name="subDomain", required, duplicate="subDomain")                      
                                                span.add-on .invoize.com
                                                label.help-inline.required(ng-show="form.subDomain.$error.required") *
                                        label.help-inline.required(ng-show="form.subDomain.$error.duplicate") Domain exists.
                        .control-group
                                label.control-label First Name
                                .controls
                                        input(type="text", name="firstName", ng-model="org.firstName", placeholder="First Name", required)
                                        label.help-inline.required(ng-show="form.firstName.$error.required") *
                        .control-group
                                label.control-label Last Name
                                .controls
                                        input(type="text", name="lastName", ng-model="org.lastName", placeholder="Last Name", required)
                                        label.help-inline.required(ng-show="form.lastName.$error.required") *
                        .control-group
                                label.control-label Email
                                .controls
                                        input(type="email", name="email", ng-model="org.email", duplicate="email", placeholder="Email", required)
                                        label.help-inline.required(ng-show="form.email.$error.required") *
                                        label.help-inline.required(ng-show="form.email.$error.email") Invalid email...!
                                        label.help-inline.required(ng-show="form.email.$error.duplicate") Email exists.
                        .control-group
                                label.control-label Account Owner
                                .controls
                                        input(type="text", name="accountOwner", ng-model="org.accountOwner", placeholder="Account Owner", required)
                                        label.help-inline.required(ng-show="form.accountOwner.$error.required") *
                        .control-group
                                label.control-label Billing Owner
                                .controls
                                        input(type="text", name="billingOwner", ng-model="org.billingOwner", placeholder="Billing Owner", required)
                                        label.help-inline.required(ng-show="form.billingOwner.$error.required") *
                        .control-group
                                label.control-label Is Active
                                .controls
                                        input(type="text", name="isActive", ng-model="org.isActive", placeholder="Is Active", required)
                                        label.help-inline.required(ng-show="form.isActive.$error.required") *
