                                                                        i.icon-chevron-down.green(ng-show="isRowCollapsed")                                           
                                                                        i.icon-chevron-up.green(ng-hide="isRowCollapsed")    

                                                                td(ng-repeat="d in data" ,ng-show="!$first")
                                                                    span.trim-info-large(tooltip="{{d |toHumanReadable}}") {{d |toHumanReadable}}
                                                        tr(ng-hide="isRowCollapsed")
